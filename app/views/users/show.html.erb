<p id="notice"><%= notice %></p>

<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<p>
  <strong>First name:</strong>
  <%= @user.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @user.last_name %>
</p>

<p>
  <strong>Profile img:</strong><br>
  <img src="<%= @user.profile_img_url %>" alt="" class="profileImg" />
</p>

<!-- <p>
  <strong>Profile img url:</strong>
  <%= @user.profile_img_url %>
</p> -->


<p>
  <strong>Bio:</strong>
  <%= @user.bio %>
</p>

<%if (session[:user_id] == @user.id || session[:user_id] == 1)%>
  <%= link_to 'Edit', edit_user_path(@user) %> |
<%end%>

<%= link_to 'Back', request.referer.present? ? request.referer : root_path%>
